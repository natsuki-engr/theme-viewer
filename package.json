{
  "name": "theme-view",
  "displayName": "theme-view",
  "description": "",
  "version": "0.0.1",
  "publisher": "Natsuki",
  "repository": {
    "type": "git",
    "url": "https://github.com/natsuki-engr/theme-viewer"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "theme-view.viewThemes",
        "title": "Open viewer"
      }
    ]
  },
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "package": "npm run check-types && node esbuild.js --production && vsce package",
    "prepare": "husky init"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/plist": "^3.0.5",
    "@types/vscode": "^1.89.0",
    "@types/vscode-webview": "^1.57.5",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "typescript": "^5.4.5",
    "vscode-webview": "^1.0.1-beta.1"
  },
  "dependencies": {
    "json5": "^2.2.3",
    "plist": "^3.1.0"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix",
      "prettier --write"
    ],
    "webview/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix --config webview/.eslintrc.cjs",
      "prettier --write"
    ]
  }
}
